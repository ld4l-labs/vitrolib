        },
        {
            "@id": "customform:dimension3FieldOptions",
            "@type": [
                "forms:ConstantList",
                "forms:FieldOptions"
             ]
        },
        {
            "@id": "customform:dependencies",
            "@type": [
                "forms:FormComponent",
                "forms:FieldDependencies"
            ],     
            "customform:dependencies": ["measurement3, value3, unit3, dimension3"]
        }  
    ]
}
