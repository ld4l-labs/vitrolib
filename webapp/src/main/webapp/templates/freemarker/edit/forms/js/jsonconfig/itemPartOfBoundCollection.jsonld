{ 
    "@context": {
        "customform": "http://vitro.mannlib.cornell.edu/ns/vitro/CustomFormConfiguration#",
        "forms": "java:edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.forms#"
    },
    "@graph": [
        {
            "@id": "customform:partOfBoundCollection_requiredN3",
            "@type": [
                "forms:RequiredN3Pattern",
                "forms:FormComponent"
            ],
            "customform:pattern": [
            
                "?subject ?predicate ?objectVar . ",
        
                "?objectVar a arm:BoundCollection , bf:Item . ",
                "?objectVar bf:hasPart ?subject . " ,
                "?objectVar bf:title ?objectVarTitle . ",
                "?objectVar rdfs:label ?titleValue . ",
                "?objectVar ch:hasCustodialHistory ?custodialHistory . ",
        
                "?objectVarTitle arm:titleOf ?objectVar . ",
                "?objectVarTitle rdf:type bf:Title . ",   
                "?objectVarTitle rdfs:label ?titleValue . ", 
                "?objectVarTitle rdf:value ?titleValue . ",
                
                "?custodialHistory a ch:CustodialHistory . ",
                "?custodialHistory ch:isCustodialHistoryOf ?subject . "
            ],
            "customform:prefixes": "@prefix arm: <https://w3id.org/arm/core/ontology/0.1/> . @prefix bf: <http://id.loc.gov/ontologies/bibframe/> . @prefix ch: <https://w3id.org/arm/custodial_history/ontology/0.1/> . @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . "
        },
        {
            "@id": "customform:partOfBoundCollection_objectVarTitle",
            "@type": [
                "forms:FormComponent",
                "forms:UriField"
            ],
            "customform:varName": "objectVarTitle",
            "customform:mayUseNewResource": true,
            "customform:queryForExistingValue": "PREFIX arm: <https://w3id.org/arm/core/ontology/0.1/> PREFIX bf: <http://id.loc.gov/ontologies/bibframe/> SELECT ?objectVarTitle WHERE { ?subject ?predicate ?objectVar . ?objectVar a arm:BoundCollection ; bf:title ?objectVarTitle . } "
        },
        {
            "@id": "customform:partOfBoundCollection_titleValue",
            "@type": [
                "forms:FormComponent",
                "forms:LiteralField"
             ],
            "customform:varName": "titleValue",
            "customform:queryForExistingValue": "PREFIX arm: <https://w3id.org/arm/core/ontology/0.1/> PREFIX bf: <http://id.loc.gov/ontologies/bibframe/>  PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> SELECT ?titleValue WHERE { ?subject ?predicate ?objectVar . ?objectVar a arm:BoundCollection ; bf:title ?objectVarTitle . ?objectVarTitle rdf:value ?titleValue . } "     
        },
        {
            "@id": "customform:partOfBoundCollection_custodialHistory",
            "@type": [
                "forms:FormComponent",
                "forms:UriField"
             ],
            "customform:varName": "custodialHistory",
            "customform:mayUseNewResource": true, 
            "customform:queryForExistingValue": "PREFIX arm: <https://w3id.org/arm/core/ontology/0.1/> PREFIX ch: <https://w3id.org/arm/custodial_history/ontology/0.1/> SELECT ?custodialHistory WHERE { ?subject ?predicate ?objectVar . ?objectVar a arm:BoundCollection ; ch:hasCustodialHistory ?custodialHistory . } "            
        }
    ]
}
