      ]
    },
     {
      "@id": "customform:noteTypeFieldOptions",
      "@type": [
        "forms:ConstantList",
        "forms:FieldOptions"
      ]
    },
    {
      "@id": "customform:activityTypeFieldOptions",
      "@type": [
        "forms:ConstantList",
        "forms:FieldOptions"
      ]
    },
     {
          "@id": "customform:dependencies",
          "@type": [
            "forms:FormComponent",
            "forms:FieldDependencies"
          ],
         
          "customform:dependencies": ["note,noteValue", "agent, agentName", "location, locationName"]
    }
   
   ]
}