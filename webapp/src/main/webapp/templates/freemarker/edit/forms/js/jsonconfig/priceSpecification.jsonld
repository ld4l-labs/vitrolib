{ 
    "@context": {
        "customform": "http://vitro.mannlib.cornell.edu/ns/vitro/CustomFormConfiguration#",
        "forms": "java:edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.forms#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "schema": "http://schema.org",
        "xsd": "http://www.w3.org/2001/XMLSchema#"  
    },
    "@graph": [
        {
            "@id": "customform:priceSpecification_requiredN3",
            "@type": [
                "forms:RequiredN3Pattern",
                "forms:FormComponent"
            ],
            "customform:pattern": [
            
                "?subject ?predicate ?objectVar . ",
                        
                "?objectVar a schema:PriceSpecification . ",
                "?objectVar schema:price ?price . ",
                "?objectVar schema:priceCurrency ?priceCurrency . "
            ],
            "customform:prefixes": "@prefix schema: <http://schema.org/> . "
        },
        {
            "@id": "customform:priceSpecification_priceSpecification",
            "@type": [
                "forms:FormComponent",
                "forms:UriField"
            ],
            "customform:varName": "objectVar",
            "customform:mayUseNewResource": true,
            "customform:queryForExistingValue": "PREFIX schema: <http://schema.org> SELECT ?priceSpecification WHERE {  ?subject ?predicate ?priceSpecification . ?priceSpecification a schema:PriceSpecification . } " 
        },
        {
            "@id": "customform:priceSpecification_price" ,
            "@type": [
                "forms:FormComponent",
                "forms:LiteralField"
             ],
             "customform:rangeDatatypeUri": {
                "@id": "xsd:decimal"
            },
            "customform:varName": "price",
            "customform:queryForExistingValue": "PREFIX schema: <http://schema.org> SELECT ?priceSpecification WHERE {  ?subject ?predicate ?priceSpecification . ?priceSpecification a schema:PriceSpecification ; schema:price ?price . } " 
        },
        {
            "@id": "customform:priceSpecification_priceCurrency" ,
            "@type": [
                "forms:FormComponent",
                "forms:LiteralField"
             ],
            "customform:varName": "priceCurrency",
            "customform:queryForExistingValue": "PREFIX schema: <http://schema.org> SELECT ?priceCurrency WHERE {  ?subject ?predicate ?priceSpecification . ?priceSpecification a schema:PriceSpecification ; schema:priceCurrency ?priceCurrency . } " 
        },
        {
          "@id": "customform:priceSpecification_dependencies",
          "@type": [
            "forms:FormComponent",
            "forms:FieldDependencies"
          ],         
          "customform:dependencies": ["priceSpecification, price, priceCurrency"]
        }
    ]
}
