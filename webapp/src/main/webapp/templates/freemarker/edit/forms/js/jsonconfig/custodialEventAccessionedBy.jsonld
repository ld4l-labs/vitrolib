{ 
    "@context": {
        "arm": "https://w3id.org/arm/core/ontology/0.1/",
        "bf": "http://id.loc.gov/ontologies/bibframe/",
        "ch": "https://w3id.org/arm/custodial_history/ontology/0.1/",
        "customform": "http://vitro.mannlib.cornell.edu/ns/vitro/CustomFormConfiguration#",
        "forms": "java:edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.forms#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "status": "https://w3id.org/arm/core/vocabularies/status/0.1/",
        "xsd": "http://www.w3.org/2001/XMLSchema#"  
    },
    "@graph": [
        {
            "@id": "customform:accessionedBy_requiredN3",
            "@type": [
                "forms:RequiredN3Pattern",
                "forms:FormComponent"
            ],
            "customform:pattern": [
            
                "?subject ?predicate ?objectVar . ",
                        
                "?objectVar a arm:AccessionNumber , bf:Identifier . ",
                "?objectVar rdf:value ?value . ",
                "?objectVar arm:accessions ?subject . ",
                
                "?objectVar bf:identifies ?item . ",
                "?item bf:identifiedBy ?objectVar . ",
            ],
            "customform:prefixes": "@prefix arm: <https://w3id.org/arm/core/ontology/0.1/> . @prefix bf: <http://id.loc.gov/ontologies/bibframe/> . @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . "
        },
        {
            "@id": "customform:accessionedBy_accessionNumber",
            "@type": [
                "forms:FormComponent",
                "forms:UriField"
            ],
            "customform:varName": "objectVar",
            "customform:mayUseNewResource": true,
            "customform:queryForExistingValue": "PREFIX arm: <https://w3id.org/arm/core/ontology/0.1/> SELECT ?accessionNumber WHERE { ?subject ?predicate ?accessionNumber . ?accessionNumber a arm:AccessionNumber . }"
        },
        {
            "@id": "customform:accessionedBy_value" ,
            "@type": [
                "forms:FormComponent",
                "forms:LiteralField"
             ],
            "customform:varName": "value",
            "customform:queryForExistingValue": "PREFIX arm: <https://w3id.org/arm/core/ontology/0.1/> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> SELECT ?value WHERE { ?subject ?predicate ?accessionNumber . ?accessionNumber a arm:AccessionNumber ; rdf:value ?value . }"
        },
        {
            "@id": "customform:accessionNumber_item" ,
            "@type": [
                "forms:FormComponent",
                "forms:UriField"
             ],
            "customform:varName": "item",
            "customform:mayUseNewResource": false,
            "customform:queryForExistingValue": "bf: <http://id.loc.gov/ontologies/bibframe/> PREFIX ch: <https://w3id.org/arm/custodial_history/ontology/0.1/> SELECT ?item WHERE { ?item a bf:Item ; ch:hasCustodialHistory ?custodialHistory . ?subject bf:partOf ?custodialHistory . }"
        },
        
        {
            "@id": "customform:accessionedBy_optionalN3",
            "@type": [
                "forms:OptionalN3Pattern",
                "forms:FormComponent"
            ],
            "customform:pattern": [
            
                "?objectVar bf:status ?status . ",
            ],
            "customform:prefixes": "@prefix bf: <http://id.loc.gov/ontologies/bibframe/> . "
        },
        {
            "@id": "customform:accessionedBy_status",
            "@type": [
                "forms:FormComponent",
                "forms:UriField"
            ],
            "customform:varName": "status",
            "customform:fieldOptons": {
                "@id": "customform:statusFieldOptions"
            },
            "customform:mayUseNewResource": false,
            "customform:queryForExistingValue": "PREFIX bf: <http://id.loc.gov/ontologies/bibframe/> PREFIX status: <https://w3id.org/arm/core/vocabularies/status/0.1/> SELECT ?status WHERE { ?objectVar bf:status ?status . ?status a bf:Status . }"
        },
        {
            "@id": "customform:statusFieldOptions",
            "@type": [
                "forms:ConstantList",
                "forms:FieldOptions"            
            ]
        }  
    ]
}
