@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://id.loc.gov/ontologies/bibframe/Audio>
      rdfs:subClassOf
      [ a       owl:Restriction ;
	owl:onProperty <http://schema.org/duration> ;
	owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string> 
      ] .
              
<http://id.loc.gov/ontologies/bibframe/Instance>
      rdfs:subClassOf
      [ a       owl:Restriction ;
	owl:onProperty <http://id.loc.gov/ontologies/bibframe/editionStatement> ;
	owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string> 
      ] .
              
<http://id.loc.gov/ontologies/bibframe/Instance>
      rdfs:subClassOf
      [ a       owl:Restriction ;
	owl:onProperty <http://id.loc.gov/ontologies/bibframe/provisionActivityStatement> ;
	owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string> 
      ] .
      
<http://measurement.bibliotek-o.org/Measurement> 
 rdfs:subClassOf
      [ a       owl:Restriction ;
	owl:onProperty rdf:value ;
	owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string> 
      ] .