<?xml version="1.0" encoding="UTF-8"?>

<!-- TODO Add/remove namespaces as relevant -->
<rdf:RDF 
    xmlns="http://example.org/"
    xmlns:bib="http://example.org/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"  
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    > 
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // ONTOLOGY DECLARATION
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <owl:Ontology rdf:about="http://example.org/"> <!-- update domain when known -->
        <owl:ontologyIRI rdf:resource="http://example.org/"/> <!-- update domain when known -->
        <owl:versionIRI rdf:resource="http://example.org/"/>   <!-- update domain and version number when known -->
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version 0.1</owl:versionInfo>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
        <rdfs:label xml:lang="en">Rare Materials Ontology</rdfs:label>
        <rdfs:comment xml:lang="en">The Rare Materials Ontology is an extension to the BIBFRAME ontology for modeling rare materials metadata.</rdfs:comment>
        <vann:preferredNamespacePrefix>rare</vann:preferredNamespacePrefix>
        <dcterms:title xml:lang="en">Rare Materials Ontology</dcterms:title>
        <voaf:extends rdf:resource="http://id.loc.gov/ontologies/bibframe/"/>
        <voaf:specializes rdf:resource="http://id.loc.gov/ontologies/bibframe/"/>
    </owl:Ontology>
    
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // CLASSES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <owl:Class rdf:about="http://example.org/AccessionNumber">
        <rdfs:label xml:lang="en">Accession number</rdfs:label>
        <skos:definition xml:lang="en">Numeric, alphanumeric, or other identifying codes assigned when an art object, book, or other item enters the collection of a museum, library, or other repository. Such codes are unique within the set of codes, and specifically identify the particular item at hand. The numbers may be marked on the objects or not. </skos:definition>        
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <skos:editorialNote xml:lang="en">Deprecate this class when it is defined in BIBFRAME.</skos:editorialNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Activity">
        <rdfs:label xml:lang="en">Activity</rdfs:label>    
        <skos:definition xml:lang="en">An activity or contribution by a single agent that affects or alters the existence or state of a resource.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Arrangement">
        <rdfs:label xml:lang="en">Arrangement</rdfs:label>    
        <skos:definition xml:lang="en">Information about the organization and arrangement of a collection of items or a single item.</skos:definition>
        <skos:scopeNote xml:lang="en">An Arrangement may be a NamedIndividual, a term from a controlled vocabulary, or an unnamed resource with an rdf:value.</skos:scopeNote>        
        <skos:example xml:lang="en">For computer files, organization and arrangement information may be the file structure and sort sequence of a file.</skos:example>
        <skos:example xml:lang="en">For visual materials, this information may be how a collection is arranged.</skos:example>
        <skos:example xml:lang="en">For an individual book on display in an exhibition, the arrangement may be the page to which the book is opened for display.</skos:example>
        <skos:example xml:lang="en">For a scroll, the arrangement may be rolled or unrolled.</skos:example>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/ArtistActivity">
        <rdfs:label xml:lang="en">Artist</rdfs:label>    
        <skos:definition xml:lang="en">The activity of creating a work by conceiving, and implementing, an original graphic design, drawing, painting, etc.</skos:definition> 
        <skos:scopeNote>For book illustrators, prefer IllustratorActivity.</skos:scopeNote>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/art.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Attribution">
        <rdfs:label xml:lang="en">Attribution</rdfs:label>    
        <skos:definition xml:lang="en">The activity of ascribing a work to a particular agent or agents. This attribution may change over time based on new evidence.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Autograph">
        <rdfs:label xml:lang="en">Autograph</rdfs:label>
        <skos:definition xml:lang="en">A person's name written on a resource in his or her own hand as part of the creation or production process.</skos:definition>        
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <skos:editorialNote xml:lang="en">As currently defined, excludes post-production autographs on an item, such as on a gift. This distinction is subject to future review.</skos:editorialNote>
        <skos:example xml:lang="en">A binder's signature</skos:example>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/BinderActivity">
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <rdfs:label xml:lang="en">Binder</rdfs:label>
        <skos:definition xml:lang="en">The activity of attaching a Binding to a resource.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">Both instances and items can have Bindings, but generally there is need to describe this activity only in relation to an item</skos:scopeNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/bnd.</skos:scopeNote>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/BindersTicket">
        <rdfs:label xml:lang="en">Binder's ticket</rdfs:label>
        <skos:definition xml:lang="en">Small engraved or printed label, usually found on the upper outside corner of one of the front flyleaves, giving the name and usually the address of the bookbinder; common in the 18th and early 19th centuries.</skos:definition>        
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/Binding">
        <rdfs:subClassOf rdf:resource="http://example.org/Enclosure"/>
        <rdfs:label xml:lang="en">Binding</rdfs:label>
        <skos:definition xml:lang="en">Covering of a book which holds the sheets together to provide protection, and collectively all the parts of a book applied in the process of attaching the leaves and covering them.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/BoundCollection">
        <rdfs:label xml:lang="en">Bound collection</rdfs:label>
        <skos:definition xml:lang="en">An artificial gathering of two or more items physically into or on a single carrier, whether bound, affixed, or otherwise brought together into a single bound unit, and lacking a collective title. Includes only collections gathered subsequent to publication, rather than issued or reissued together by the publisher.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/CaptionNote">
        <rdfs:label xml:lang="en">Caption note</rdfs:label>
        <skos:definition xml:lang="en">A distinctive caption independent of the resource title, including mottos and headings.</skos:definition>
        <rdfs:comment xml:lang="en">Use separate notes for multiple captions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/Citation">
        <rdfs:label xml:lang="en">Citation</rdfs:label>
        <skos:definition xml:lang="en">A single citation within a bibliographic source.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/ColoristActivity">
        <rdfs:label xml:lang="en">Colorist</rdfs:label>    
        <skos:definition xml:lang="en">The activity of applying color to drawings, prints, photographs, maps, moving images, etc.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/clr.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/ConditionAssessment">
        <rdfs:label xml:lang="en">Condition assessment</rdfs:label>
        <skos:definition xml:lang="en">Assessment and/or documentation of the physical condition of an item, including damage, material vulnerabilities, special storage requirements, etc. The assessment is generally the output of a condition assessment activity, and may or may not result in a conservator activity.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/ConditionAssessmentActivity">
        <rdfs:label xml:lang="en">Condition assessment activity</rdfs:label>
        <skos:definition xml:lang="en">The activity of assessing and documenting the physical condition of an item, including damage, material vulnerabilities, special storage requirements, etc. This activity outputs a ConditionAssessment.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/ConservatorActivity">
        <rdfs:label xml:lang="en">Conservator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of preserving or treating printed or manuscript material, works of art, artifacts, or other objects.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/con.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/ContentsNote">
        <rdfs:label xml:lang="en">Contents note</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <skos:definition xml:lang="en">A note to provide a main contents of the resource rather than linking to an external table of contents or creating separate part/whole relationships to the entities that comprise the whole. Ideally, part whole relationships would be created instead of the this type of note.</skos:definition>
        <skos:editorialNote>This note type was created based on http://bibliotek-o.org/1.1/ontology/specifyingContents. Likely belongs in a general bf:Notes extension, but need to add it here for now.</skos:editorialNote>
        <dcterms:issued>2018-01-29T00:00:00-04:00</dcterms:issued>
    </owl:Class> 
    
    <owl:Class rdf:about="http://example.org/CreatorActivity">
        <rdfs:label xml:lang="en">Creator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of creating a resource.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/cre.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/CuratorActivity">
        <rdfs:label xml:lang="en">Curator</rdfs:label>    
        <skos:definition xml:lang="en">The activity of conceiving, aggregating, and/or organizing an exhibition, collection, or other item.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/cur.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/DescriptiveNote">
        <rdfs:label xml:lang="en">Descriptive note</rdfs:label> 
        <skos:definition xml:lang="en">A note that describes the resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/DesignerActivity">
        <rdfs:label xml:lang="en">Designer</rdfs:label> 
        <skos:definition xml:lang="en">The activity of creating a design for an object.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/dsr.</skos:scopeNote>     
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/DirectorActivity">
        <rdfs:label xml:lang="en">Director</rdfs:label>    
        <skos:definition xml:lang="en">The activity of general management and supervision of a filmed performance, a radio or television program, etc.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/drt.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/DraftsmanActivity">
        <rdfs:label xml:lang="en">Draftsman</rdfs:label>    
        <skos:definition xml:lang="en">The activity of making detailed plans or drawings for buildings, ships, aircraft, machines, objects, etc.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/drm.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/DustJacket">
        <rdfs:subClassOf rdf:resource="http://example.org/Enclosure"/>
        <rdfs:label xml:lang="en">DustJacket</rdfs:label>
        <skos:definition xml:lang="en">A detachable, flexible covering for a book, usually of paper cut flush with the covers, folding around the binding with the ends tucked in between the cover boards and the free end paper.</skos:definition>    
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Enclosure">
        <rdfs:label xml:lang="en">Enclosure</rdfs:label>
        <skos:definition xml:lang="en">Covering of a book which holds the sheets together to provide protection, and collectively all the parts of a book applied in the process of attaching the leaves and covering them.</skos:definition>
        <skos:scopeNote xml:lang="en">This class includes containers, such as a box containing a paper doll set.</skos:scopeNote>  
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/EngraverActivity">
        <rdfs:label xml:lang="en">Engraver</rdfs:label>    
        <skos:definition xml:lang="en">The activity of cutting into a surface, such as a wooden or metal plate used for printing.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/egr.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Entry">
        <rdfs:label xml:lang="en">Entry</rdfs:label>
        <skos:definition xml:lang="en">A specific entry within a bibliographic resource.</skos:definition>
        <skos:example xml:lang="en">An entry locating a citation within a bibliographic source.</skos:example>
        <skos:example xml:lang="en">An exhibition catalog entry describing a specific item in the exhibition.</skos:example>       
        <skos:scopeNote xml:lang="en">Used in parsing out hierarchically structured location designators for citations.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/EtcherActivity">
        <rdfs:label xml:lang="en">Etcher</rdfs:label>    
        <skos:definition xml:lang="en">The activity of producing text or images for printing by subjecting metal, glass, or some other surface to acid or the corrosive action of some other substance.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/etr.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Exhibition">
        <rdfs:label xml:lang="en">Exhibition</rdfs:label>
        <skos:definition xml:lang="en">The relationship between an ExhibitionEvent and an Item in that event.</skos:definition>
        <skos:scopeNote xml:lang="en">Used to attach information to this relationship, such as title, dates, activities such as curatorship, exhibit number, physical arrangement, etc. which does not properly attach to the Item in and of itself.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Font">
        <rdfs:label xml:lang="en">Font</rdfs:label>
        <skos:definition xml:lang="en">A font is the digital representation of a typeface. It is a collection of all the characters of a typeface in one size (12pt/1in) and one style (bold/italic). For example, Arial in 12pt size is a font, Arial in 18pt size is a font, and Arial bold in 24pt is also a font.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/FontStyle">
        <rdfs:label xml:lang="en">Font style</rdfs:label>
        <skos:definition xml:lang="en">Style of the font, e.g. bold, italic</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Frame">
        <rdfs:subClassOf rdf:resource="http://example.org/Mount"/>
        <rdfs:label xml:lang="en">Frame</rdfs:label>
        <skos:definition xml:lang="en">A supportive structure that surrounds or encloses an item.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Handwriting">
        <rdfs:label xml:lang="en">Handwriting</rdfs:label>
        <skos:definition xml:lang="en">A specific example of a handwriting found in a textual resource. A handwriting can have a particular HandWritingType.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/HandwritingType">
        <rdfs:label xml:lang="en">Handwriting type</rdfs:label>
        <skos:definition xml:lang="en">A type of writing hand; not the same as a writing system or a handwriting style (such as cursive or block letters). Examples include Gothic, Secretary, Carolingian minuscule, and Sütterlin.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/InaccuracyNote">
        <rdfs:label xml:lang="en">Inaccuracy note</rdfs:label>
        <skos:definition xml:lang="en">A note explaining an unintentional or intentional inaccuracy, misspelling, or misprint relating to a specific resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/note"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Inscription">
        <rdfs:label xml:lang="en">Inscription</rdfs:label>
        <skos:definition xml:lang="en">Words, texts, lettering, or symbols written in manuscript on a resource.</skos:definition>        
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Label">
        <rdfs:label xml:lang="en">Label</rdfs:label>
        <skos:definition xml:lang="en">A piece of paper, leather, fabric, or other material inscribed, printed, or engraved with information and affixed to a resource or part of a resource for identification or description.</skos:definition>        
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/LimitationNote">
        <rdfs:label xml:lang="en">Limitation note</rdfs:label>
        <skos:definition xml:lang="en">A note relating to the size and nature of a print run limited to a specific quantity of copies produced, or a statement relating to the enumeration and nature of a copy in relation to the larger print run.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/LithographerActivity">
        <rdfs:label xml:lang="en">Lithographer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of preparing the stone or plate for lithographic printing, including a graphic artist creating a design directly on the surface from which printing will be done.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/ltg.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/MarblerActivity">
        <rdfs:label xml:lang="en">Marbler</rdfs:label>    
        <skos:definition xml:lang="en">The activity of marbling paper, cloth, leather, etc. used in construction of a resource.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/mrb.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Marking">
        <rdfs:label xml:lang="en">Marking</rdfs:label>
        <skos:definition xml:lang="en">A standardized symbol, notation, or other marking present in or applied to the materials of which an object is composed, or applied to an object during or after creation, conveying information such as the object's origin or maker, its authenticity, or a change in its official status.</skos:definition>
        <skos:editorialNote xml:lang="en">As currently defined, contrasts with marginialia and other unofficial markings applied by individual users outside the creation or production process or custodial history of an object. This distinction may at times be subtle, and is subject to future review.</skos:editorialNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 

    <owl:Class rdf:about="http://example.org/Mount">
        <rdfs:label xml:lang="en">Mount</rdfs:label>
        <skos:definition xml:lang="en">An item placed on a pedestal or a case, or otherwise attached to a print, photographic image, or document protected by framing, usually against a backing material.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 
    
    <owl:Class rdf:about="http://example.org/OrganizerActivity">
        <rdfs:label xml:lang="en">Organizer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of organizing an exhibit, event, conference, etc.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/orm.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Page">
        <rdfs:label xml:lang="en">Page</rdfs:label>
        <skos:definition xml:lang="en">A single page within a resource.</skos:definition>
        <rdfs:comment xml:lang="en">Used in any page-level description, as well as in parsing out hierarchically structured location designators for citations.</rdfs:comment>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/PaginationFoliation">
        <rdfs:label xml:lang="en">Pagination foliation</rdfs:label>
        <skos:definition xml:lang="en">Sequential numbering of individual leaves, plates, pages (numbering on both sides), or columns (used as a sequence of numbering) of a resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Extent"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Pedestal">
        <rdfs:subClassOf rdf:resource="http://example.org/Mount"/>
        <rdfs:label xml:lang="en">Pedestal</rdfs:label>
        <skos:definition xml:lang="en">A solid, fixed base or support on which an item or part of an item is mounted.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/PhotographerActivity">
        <rdfs:label xml:lang="en">Photographer</rdfs:label>    
        <skos:definition xml:lang="en">The activity of creating a photographic work.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/pht.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/PhysicalCondition">
        <rdfs:label xml:lang="en">Physical Condition</rdfs:label>
        <skos:definition xml:lang="en">One specific aspect of the condition of an object or part of an object, including perceptible or measurable characteristics such as physical appearance, level of damage, quality, or completeness.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/PreferredCitation">
        <rdfs:label xml:lang="en">Preferred citation</rdfs:label>
        <skos:definition xml:lang="en">A note providing a specific citation format for citing the resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/PrintmakerActivity">
        <rdfs:label xml:lang="en">Printmaker</rdfs:label>    
        <skos:definition xml:lang="en">The activity of making a relief, intaglio, or planographic printing surface.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/prm.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/RelatedMaterialNote">
        <rdfs:label xml:lang="en">Related material note</rdfs:label>
        <skos:definition xml:lang="en">A note providing information on materials (e.g. archival materials or publications) related to the described resource.</skos:definition>
        <skos:scopeNote xml:lang="en">Use if information describing the related material cannot be parsed into the appropriate relationship.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>     
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/SculptorActivity">
        <rdfs:label xml:lang="en">Sculptor</rdfs:label>    
        <skos:definition xml:lang="en">The activity of creating a three-dimensional work by modeling, carving, or similar technique.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/scl.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Seal">
        <rdfs:label xml:lang="en">Seal</rdfs:label>
        <skos:definition xml:lang="en">An impression that has been made in wax, clay, or another soft material by the impact of a hard engraved surface with monograms, writing, numbers, symbols, or design in intaglio, generally used to authenticate documents or for decoration.</skos:definition>        
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 
    
    <owl:Class rdf:about="http://example.org/SignatureStatement">
        <rdfs:label xml:lang="en">Signature statement</rdfs:label>
        <skos:definition xml:lang="en">A formulaic statement describing the pattern followed in assembling the sheets of the book, as expressed in its signatures. Signatures are letters, numerals, symbols, or a group of such characters printed at the foot of the rectos of the first few leaves of each gathering (quire) to aid the binder in correctly assembling the sections.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/SlipCase">
        <rdfs:subClassOf rdf:resource="http://example.org/Enclosure"/>
        <rdfs:label xml:lang="en">SlipCase</rdfs:label>
        <skos:definition xml:lang="en">Protective container designed to protect a book or item with open end along one edge.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 
    
    <owl:Class rdf:about="http://example.org/SponsorActivity">
        <rdfs:label xml:lang="en">Sponsor</rdfs:label>    
        <skos:definition xml:lang="en">The activity of sponsoring some aspect of a resource, e.g., funding research, sponsoring an event.</skos:definition>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/spn.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://example.org/Activity"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/Stamp">
        <rdfs:label xml:lang="en">Stamp</rdfs:label>
        <skos:definition xml:lang="en">A mark that is impressed, printed, perforated, or embossed, made by a hard object, especially a die, block, or other tool, on a softer material such as paper or wax.</skos:definition>        
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>   
    
    <owl:Class rdf:about="http://example.org/SubjectNote">
        <rdfs:label xml:lang="en">Subject note</rdfs:label>
        <skos:definition xml:lang="en">Textual information describing the subject or iconographic details of a resource.</skos:definition>
        <skos:scopeNote xml:lang="en">Use for textual notes only, not for subject terms from a controlled value vocabulary.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/SummaryNote">
        <rdfs:label xml:lang="en">Summary note</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <skos:definition xml:lang="en">A note providing a summary of the resource.</skos:definition>
        <skos:editorialNote>This note type was created based on http://bibliotek-o.org/1.1/ontology/summarizing. Likely belongs in a general bf:Notes extension, but need to add it here for now.</skos:editorialNote>
        <dcterms:issued>2018-01-29T00:00:00-04:00</dcterms:issued>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/SupplementaryContentNote">
        <rdfs:label xml:lang="en">Supplementary content note</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
        <skos:definition xml:lang="en">A note indicating the existence of supplementary content such as bibliographies, discographies, filmographies, and/or other bibliographic references in the described resource or in accompanying material.</skos:definition>
        <skos:editorialNote>This note type was created based on http://bibliotek-o.org/1.1/ontology/describingSupplementaryContent. Likely belongs in a general bf:Notes extension, but need to add it here for now.</skos:editorialNote>
        <dcterms:issued>2018-01-29T00:00:00-04:00</dcterms:issued>
    </owl:Class>
 
    <owl:Class rdf:about="http://example.org/TextBlock">
        <owl:disjointWith rdf:resource="http://example.org/Enclosure"/>
        <rdfs:label xml:lang="en">Text block</rdfs:label>
        <skos:definition xml:lang="en">Those parts of a book consisting of the stack of leaves or gatherings to be bound, exclusive of the materials, such as endpapers or boards, that are added in binding.</skos:definition>
        <skos:scopeNote xml:lang="en">The measurements of a book may include the binding or not. A text block resource can be used in the specification of measurements exclusive of the binding.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 
    
    <owl:Class rdf:about="http://example.org/Typeface">
        <rdfs:label xml:lang="en">Typeface</rdfs:label>
        <skos:definition xml:lang="en">A design or “font family” created by a typographer or type designer. It defines specific stylistic features to determine the type display of a writing system.</skos:definition>
        <rdfs:comment xml:lang="en">BIBFRAME uses bf:Script to refer to writing systems</rdfs:comment>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Volume">
        <rdfs:label xml:lang="en"></rdfs:label>
        <skos:definition xml:lang="en">A single bibliographic or physical volume of a resource.</skos:definition>
        <rdfs:comment xml:lang="en">Used in any volume-level description, as well as in parsing out hierarchically structured location designators for citations.</rdfs:comment>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/Watermark">
        <rdfs:label xml:lang="en">Watermark</rdfs:label>
        <skos:definition xml:lang="en">A faint design or device incorporated into paper as it is made and appearing as translucent or shaded areas visual only through transmitted light. Often used to give the name of the manufacturer and the date of the paper's manufacture; a symbol, seal, or initials are also common. Often used to detect and prevent counterfeiting and forgery.</skos:definition>     
        <rdfs:subClassOf rdf:resource="http://example.org/Marking"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class>   
    
    <owl:Class rdf:about="http://example.org/WrapAroundBand">
        <rdfs:subClassOf rdf:resource="http://example.org/Enclosure"/>
        <rdfs:label xml:lang="en">Wrap around band</rdfs:label>
        <skos:definition xml:lang="en">A printed paper the length of the dust jacket but smaller in height and wrapped around the book. It typically has reviews and is used for promotional purposes; may be added post-publication.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 
    
    <owl:Class rdf:about="http://example.org/Wrapper">
        <rdfs:subClassOf rdf:resource="http://example.org/Enclosure"/>
        <rdfs:label xml:lang="en">Wrapper</rdfs:label>
        <skos:definition xml:lang="en">A flexible printed or unprinted cover, usually made of paper, attached to a book or pamphlet; commonly used as an inexpensive temporary binding in the late 18th and early 19th century.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:Class> 


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // OBJECT PROPERTIES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    
    <owl:ObjectProperty rdf:about="http://example.org/agentOf">
        <rdfs:label xml:lang="en">agent of</rdfs:label>
        <skos:definition xml:lang="en">Relates an agent to the activity it participated in.</skos:definition>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Agent"/> 
        <owl:inverseOf rdf:resource="http://id.loc.gov/ontologies/bibframe/agent"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/atLocation">
        <rdfs:label xml:lang="en">at location</rdfs:label>
        <skos:definition xml:lang="en">The resource being described is at the specified location.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/isLocationOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/carries">
        <rdfs:label xml:lang="en">carries</rdfs:label>
        <skos:definition xml:lang="en">A particular physical form, such as a volume, a sheet of paper, or a computer disk, contains or has on it the resource being described.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/isOnCarrier"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/coveredIn">
        <rdfs:label xml:lang="en">covered in</rdfs:label>
        <skos:definition xml:lang="en">Specifies a resource whose content covers this time period or location.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/covers"/> 
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>  
    
    <owl:ObjectProperty rdf:about="http://example.org/covers">
        <rdfs:label xml:lang="en">covers</rdfs:label>
        <skos:definition xml:lang="en">Specifies a time period or location covered in the content of this resource.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/coveredIn"/> 
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>  
    
    <owl:ObjectProperty rdf:about="http://example.org/describedBy">
        <rdfs:label xml:lang="en">described by</rdfs:label>
        <skos:definition xml:lang="en">This resource is described by another, for example a PhysicalCondition is described by a ConditionAssessment</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/describes"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty> 
    
    <owl:ObjectProperty rdf:about="http://example.org/describes">
        <rdfs:label xml:lang="en">describes</rdfs:label>
        <skos:definition xml:lang="en">This resource provides a description of another, for example a ConditionAssessment describes a PhysicalCondition.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/describedBy"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/hasActivity">
        <rdfs:label xml:lang="en">has activity</rdfs:label>
        <skos:definition xml:lang="en">Relates this resource to an activity or contribution by a single agent that affects or alters its existence or state.</skos:definition>
        <rdfs:range rdf:resource="http://example.org/Activity"/> 
        <owl:inverseOf rdf:resource="http://example.org/isActivityOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/hasArrangement">
        <rdfs:label xml:lang="en">has arrangement</rdfs:label>
        <skos:definition xml:lang="en">Relates an object or collection of objects to information about its organization or arrangement.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/isArrangementOf"/> 
        <skos:example xml:lang="en">For computer files, organization and arrangement information may be the file structure and sort sequence of a file.</skos:example>
        <skos:example xml:lang="en">For visual materials, this information may be how a collection is arranged.</skos:example>
        <skos:example xml:lang="en">For an individual book on display in an exhibition, the arrangement may be the page to which the book is opened for display.</skos:example>
        <skos:example xml:lang="en">For a scroll, the arrangement may be rolled or unrolled.</skos:example>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>  
    
    <owl:ObjectProperty rdf:about="http://example.org/hasAttribution">
        <rdfs:label xml:lang="en">has attribution</rdfs:label>
        <skos:definition xml:lang="en">The attribution of an activity of ascribing a work to a particular agent or agents.</skos:definition>
        <rdfs:range rdf:resource="http://example.org/Attribution"/> 
        <owl:inverseOf rdf:resource="http://example.org/isAttributionOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/hasCitation">
        <rdfs:label xml:lang="en">has citation</rdfs:label>
        <skos:definition xml:lang="en">The resource that is the focus of description (e.g., a bf:Item or bf:Instance) has a citation located in a standard reference source.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/isCitationOf"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/hasExhibition">
        <rdfs:label xml:lang="en">has exhibition</rdfs:label>
        <skos:definition xml:lang="en">Relates an item to the Exhibition resource representing the relationship between the item and an ExhibitionEvent.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/isExhibitionOf"/> 
        <rdfs:range rdf:resource="http://example.org/Exhibition"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty> 
    
    <owl:ObjectProperty rdf:about="http://example.org/hasHandwritingType">
        <rdfs:label xml:lang="en">has handwriting type</rdfs:label>
        <skos:definition xml:lang="en">A relationship between a resource that includes handwriting and the type of writing hand, e.g. Gothic, Secretary, Carolingian minuscule, and Sütterlin.</skos:definition>
        <rdfs:comment xml:lang="en">Use on a :Handwriting</rdfs:comment>
        <rdfs:range rdf:resource="http://example.org/HandwritingType"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty> 
    
    <owl:ObjectProperty rdf:about="http://example.org/hasInput">
        <rdfs:label xml:lang="en">has input</rdfs:label>
        <skos:definition xml:lang="en">One resource has another resource as input, for example when one resource is informed or acted on by another. A ConservatorActivity has input from a ConditionAssessment.</skos:definition>
        <owl:inverseOf rdf:resource="http://example.org/isInputTo"/>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>  
    
    <owl:ObjectProperty rdf:about="http://example.org/hasOrigin">
        <rdfs:label xml:lang="en">has origin</rdfs:label>    
        <skos:definition xml:lang="en">Specifies the physical location from which this resource (e.g., a title) originates, such as spine, cover, container, etc.</skos:definition>
        <skos:scopeNote xml:lang="en">Distinct from http://example.org/hasSource, which specifies an external organization or scheme from which the resource was obtained.</skos:scopeNote>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/hasPhysicalCondition">
